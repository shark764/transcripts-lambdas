{
  "name": "email-transcripts",
  "version": "1.0.0-SNAPSHOT",
  "description": "This is the starter kit for a new lambdas repository.",
  "main": "index.js",
  "scripts": {
    "alonzo": "alonzo",
    "alonzo:install": "alonzo --install",
    "zip": "alonzo --zip",
    "build": "npm run alonzo:install && npm run zip",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
