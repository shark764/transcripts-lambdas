  "bugs": {
    "url": "https://github.com/SerenovaLLC/email-transcripts/issues"
  },
  "homepage": "https://github.com/SerenovaLLC/email-transcripts#readme",
  "dependencies": {},
  "devDependencies": {
    "alonzo": "^8.0.3-SNAPSHOT",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "0.14.3",
    "jest": "^24.9.0"
  }
